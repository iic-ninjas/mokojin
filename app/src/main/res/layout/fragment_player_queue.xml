<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="com.iic.mokojin.PlayerQueueFragment">

    <de.timroes.android.listview.EnhancedListView
        android:id="@+id/queue_list_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.getbase.floatingactionbutton.AddFloatingActionButton
        android:id="@+id/add_player_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:fab_plusIconColor="@color/white"
        app:fab_colorNormal="@color/fab_color"
        app:fab_colorPressed="@color/fab_color"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"/>
</RelativeLayout>
